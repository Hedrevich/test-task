<mvc:View
        xmlns="sap.uxap"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:layout="sap.ui.layout"
        xmlns:m="sap.m"
        xmlns:core="sap.ui.core"
        xmlns:f="sap.ui.layout.form"
        controllerName="sap.ui.demo.basicTemplate.controller.ProjectDetails">
    <ObjectPageLayout
            id="ObjectPageLayout"
            busyIndicatorDelay="0"
            upperCaseAnchorBar="false">
        <headerTitle>
            <ObjectPageHeader id="DetailsHeader" objectTitle="{ProjectName}">
                <actions>
                    <ObjectPageHeaderActionButton id="_IDEGen_objectpageheaderactionbutton0" text="{i18n>edit}"
                                                  hideIcon="true" hideText="false" type="Emphasized"
                                                  press="onEditPress"/>
                    <ObjectPageHeaderActionButton id="_IDEGen_objectpageheaderactionbutton2" text="{i18n>delete}"
                                                  hideIcon="true" hideText="false"
                                                  press="onDeletePress"
                    />
                </actions>
            </ObjectPageHeader>
        </headerTitle>

        <headerContent>
            <layout:VerticalLayout id="_IDEGen_verticallayout0">
                <m:Label id="_IDEGen_label0" text="{i18n>headerProjectDescription}"/>
                <m:ObjectAttribute id="_IDEGen_objectattribute0" text="{ProjectDescription}"/>
            </layout:VerticalLayout>
            <layout:VerticalLayout id="_IDEGen_verticallayout1">
                <m:Label id="_IDEGen_label1" text="{i18n>headerProjectStatus}"/>
                <m:ObjectAttribute id="_IDEGen_objectattribute1" text="{ProjectStatus}"/>
            </layout:VerticalLayout>
        </headerContent>

