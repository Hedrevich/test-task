            <ObjectPageSection id="SchedulingOptionSection" title="{i18n>projectMembers}" titleUppercase="false">
                <subSections>
                    <ObjectPageSubSection id="_IDEGen_objectpagesubsection1">
                        <core:Fragment id="SchedulingOptions"
                                       fragmentName="sap.ui.demo.basicTemplate.view.fragments.MembersList"
                                       type="XML"/>
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>

            <ObjectPageSection id="SchedulingParameterSection" title="{i18n>projectTasks}"
                               titleUppercase="false">
                <subSections>
                    <ObjectPageSubSection id="_IDEGen_objectpagesubsection2">
                        <core:Fragment id="SchedulingParameters"
                                       fragmentName="sap.ui.demo.basicTemplate.view.fragments.TaskList"
                                       type="XML"/>
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>
        </sections>

        <footer>
            <m:OverflowToolbar id="_IDEGen_overflowtoolbar0">
                <m:ToolbarSpacer id="_IDEGen_toolbarspacer0"/>
                <m:Button
                        id="footerSaveBtn"
                        type="Emphasized"
                        text="{i18n>save}"
                        press="onSaveButtonPress"
                        visible="{viewModel>/isEditMode}"/>
                <m:Button id="_IDEGen_button0" text="{i18n>cancel}" press="onCancelPress"/>
            </m:OverflowToolbar>
